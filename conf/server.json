{
    "name": "openodin-server",
    "version": "0.2",
    "peers": [
        {
            "connection": {
                "maxConnectionsPerIp": 10,
                "maxConnections": 1000,
                "server": {
                    "socketType": "WebSocket",
                    "host": "0.0.0.0",
                    "port": 1117
                }
            },
            "permissions": {
                "fetchPermissions": {
                    "allowTrigger": true,
                    "allowIncludeLicenses": 3,
                    "allowNodeTypes": ["0004"],
                    "allowReadBlob": true
                },
                "storePermissions": {
                    "allowStore": true,
                    "allowWriteBlob": true
                }
            }
        }
    ]
}
